{
  "name": "promo-calculator",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:migrate:dev": "knex --knexfile knexfile.cjs migrate:latest --env development",
    "db:rollback:dev": "knex --knexfile knexfile.cjs migrate:rollback --env development",
    "db:migrate:prod": "knex --knexfile knexfile.cjs migrate:latest --env production",
    "predev": "npm run db:migrate:dev"
  },
  "dependencies": {
    "autoprefixer": "latest",
    "next": "14.1.0",
    "pg": "^8.11.0",
    "postcss": "latest",
    "react": "latest",
    "react-dom": "latest",
    "tailwindcss": "^4.1.17",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "knex": "^3.1.0",
    "typescript": "5.9.3"
  }
}